// minimal_eliftest.silly
FUNCTION check(INT32 val)
{
    IF (val < 0)
    {
        PRINT "negative";
    }
    ELIF (val > 0)
    {
        PRINT "positive";
    }
    ELSE
    {
        PRINT "zero";
    };
    RETURN;
};

